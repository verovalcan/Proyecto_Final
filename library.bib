number = {3},
pages = {639--646},
pmid = {139},
title = {{Seasonal fluctuations in sand temperature: Effects on the incubation period and mortality of loggerhead sea turtle (Caretta caretta) pre-emergent hatchlings in Minabe, Japan}},
volume = {140},
year = {2002}
}
