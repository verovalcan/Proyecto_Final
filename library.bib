doi = {10.3354/meps12391},
file = {:C$\backslash$:/Users/verov/Documents/Publicaciones/Tortugas marinas/Gaos{\_}2018{\_}rookerie{\_}contributions{\_}movements{\_}needs{\_}hawksbills{\_}at{\_}foraging{\_}grounds.pdf:pdf},
issn = {01718630},
journal = {Marine Ecology Progress Series},
keywords = {Marine conservation,Mixed-stock analysis,Movement,Natal foraging philopatry,Nesting colonies,Rookery contributions},
