journal = {Endangered Species Research},
keywords = {Black turtle,Chelonia mydas,Costa rica,East Pacific green turtle,Foraging grounds,Migration,Satellite telemetry,Switching state-space model,black turtle,chelonia mydas,costa rica,east pacific green turtle,foraging,grounds,migration,of the publisher,permitted without written consent,resale or republication not,satellite telemetry,switching state-space model},
pages = {233--245},
title = {{Post-nesting movements and feeding grounds of a resident East Pacific green turtle {\textless}i{\textgreater}Chelonia mydas{\textless}/i{\textgreater}population from Costa Rica}},
volume = {18},
