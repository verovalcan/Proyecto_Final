publisher = {Chelonian Conservation and Biology Journal},
title = {{Ecology of Hawksbill turtles, {\textless}i{\textgreater}Eretmochelys imbricata{\textless}/i{\textgreater}, on a western Caribbean foraging ground}},
volume = {8},
year = {2009}
}
