file = {:C$\backslash$:/Users/verov/Documents/Publicaciones/Tortugas marinas/Mansfield{\_}2017{\_}sattelite{\_}tracks{\_}Lost{\_}years{\_}seasonal{\_}variation.pdf:pdf},
issn = {14712954},
journal = {Proceedings of the Royal Society B: Biological Sciences},
keywords = {Caretta caretta,Dispersal,Marine turtle oceanic stage,Ocean currents,Population connectivity,South Atlantic Ocean},
pages = {20171730},
title = {{First satellite tracks of south atlantic sea turtle ‘lost years': Seasonal variation in trans-equatorial movement}},
