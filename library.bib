@article{Mazaris2013,
abstract = {Variations in environmental parameters (e.g. temperature) that form part of global climate change have been associated with shifts in the timing of sea-sonal events for a broad range of organisms. Most studies evaluating such phenological shifts of individual taxa have focused on a limited number of locations, making it difficult to assess how such shifts vary regionally across a species range. Here, by using 1445 records of the date of first nesting for loggerhead sea turtles (Caretta caretta) at different breeding sites, on different continents and in different years across a broad latitudinal range (25 –398 0 N), we demonstrate that the gradient of the relationship between temperature and the date of first breeding is steeper at higher latitudes, i.e. the phenological responses to temperature appear strongest at the pole-ward range limit. These findings support the hypothesis that biological changes in response to climate change will be most acute at the poleward range limits and are in accordance with the predictions of MacArthur's hypothesis that poleward range limit for species range is environmentally limited. Our findings imply that the poleward populations of loggerheads are more sensitive to climate variations and thus they might display the impacts of climate change sooner and more prominently.},
author = {Mazaris, Antonios D. and Kallimanis, Athanasios S. and Pantis, John D. and Hays, Graeme C.},
doi = {10.1098/rspb.2012.2397},
file = {:C$\backslash$:/Users/verov/Documents/Publicaciones/Tortugas marinas/Mazaris{\_} 2013{\_}Phenology{\_}environmental{\_}variation{\_}northern{\_}range.pdf:pdf},
journal = {Proceedings of the Royal Society B},
keywords = {dimatic niche,geographical variation,meta-analysis,phenotypic plasticity,reaction norms},
mendeley-tags = {dimatic niche,geographical variation,meta-analysis,phenotypic plasticity,reaction norms},
pages = {20122397},
title = {{Phenological response of sea turtles to environmental variation across a species ' northern range}},
url = {http://dx.doi.org/10.1098/rspb.2012.2397},
volume = {280},
year = {2013}
}
