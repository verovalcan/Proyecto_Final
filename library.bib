number = {December},
pages = {17--31},
title = {{Din{\'{a}}mica atmosf{\'{e}}rica y oce{\'{a}}nica en algunos sitios del {\'{A}}rea de Conservaci{\'{o}}n Guanacaste (ACG), Costa Rica}},
volume = {62},
