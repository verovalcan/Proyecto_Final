isbn = {0123456789},
issn = {14321793},
journal = {Marine Biology},
pages = {106},
publisher = {Springer Berlin Heidelberg},
title = {{Determining critical inter-nesting, migratory, and foraging habitats for the conservation of East Atlantic green turtles (Chelonia mydas)}},
