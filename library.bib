keywords = {Critically endangered,Demography,Mangrove estuary,Marine conservation,Population status,Reproductive biology},
number = {3},
pages = {572--584},
title = {{Living on the edge: Hawksbill turtle nesting and conservation along the Eastern Pacific Rim}},
volume = {45},
year = {2017}
}
