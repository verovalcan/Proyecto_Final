author = {Tob{\'{o}}n-L{\'{o}}pez, Alexander and {Amorocho Llanos}, Diego Fernando},
journal = {Acta Biol{\'{o}}gica Colombiana},
keywords = {Parque Nacional Natural Gorgona,bioqu{\'{i}}mica sangu{\'{i}}nea,longitud curvo caparaz{\'{o}}n},
number = {3},
pages = {447--457},
title = {{Estudio poblacional de la tortuga carey Eretmochelys imbricata (Cheloniidae) en el pac{\'{i}}fico sur de Colombia}},
volume = {19},
year = {2014}
}
